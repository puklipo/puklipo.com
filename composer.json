{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.29",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "laravel/vapor-cli": "^1.65",
        "laravel/vapor-core": "^2.37",
        "livewire/livewire": "^3.5",
        "openai-php/laravel": "^0.10.1",
        "puklipo/laravel-vapor-gzip": "^1.3",
        "revolution/laravel-bluesky": "^0.9.0",
        "revolution/laravel-nostr": "^0.6.0",
        "revolution/laravel-notification-discord-webhook": "^1.1",
        "revolution/laravel-str-mixins": "^2.4",
        "revolution/laravel-threads": "^0.2.1",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-sitemap": "^7.2",
        "torann/json-ld": "^0.0.19"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel-lang/common": "^6.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lang:update",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sail:up": [
            "./vendor/bin/sail up -d",
            "./vendor/bin/sail art ide-helper:models -M"
        ],
        "sail:down": "./vendor/bin/sail down"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
